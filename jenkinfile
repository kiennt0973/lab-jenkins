#!/usr/bin/env groovy

pipeline {

agent any

stages {
    stage('Build') {
        steps {
            echo 'Building...'
        }
    }
    stage('Install maven') {
        steps {
            sh 'mvn install'
        }
    }
    stage('Maven Test auto') {
        steps {
            sh 'mvn test'
        }
    }
}
}